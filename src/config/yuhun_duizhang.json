{
    "module_type": "yuhun_duizhang.json-御魂(队长)",
    "start_module": {
        "describe": "御魂流程：挑战->挑战等待->胜利结算->奖励领取->挑战...",
        "start_module": "check_challenge"
    },
    "check_challenge":{
        "action": "check",
        "describe": "开始挑战",
        "template": "template/start.bmp",
        "next_action": "sleep"
    },
    "sleep":{
        "action": "sleep",
        "describe": "挑战中，请耐心等待...",
        "time_":20,
        "next_action": "check_victory"
    },
    "check_victory":{
        "action": "check",
        "describe": "胜利结算",
        "template": "template/end1.bmp,template/end2.bmp",
        "next_action": "check_rewards"
    },
    "check_rewards":{
        "action": "check",
        "describe": "奖励领取",
        "template": "template/end3.bmp",
        "next_action": "check_challenge"
    }
}